<Project Sdk="Microsoft.NET.Sdk.Razor">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <RazorLangVersion>3.0</RazorLangVersion>
    <Description>This library shows sample .razor templates and related code, used in the unit test library</Description>
    <Authors>conficient</Authors>
    <Company>conficient</Company>
    <Product>BlazorTemplater</Product>
  </PropertyGroup>

  <ItemGroup>
    <EmbeddedResource Include="ComponentWithCss.razor.css" />
    <EmbeddedResource Include="MultipleComponentsWithCss.razor.css" />
